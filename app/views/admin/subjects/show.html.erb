<div class="col-xs-12 col-sm-8">
  <div class="tt-course">
    <div class="tt-course-summary">
      <h4><%= @subject.name %></h4>
    </div>
    <div class="tt-subject">
      <p><%= t "admin.list_answer_in_subject" %></p>
      <% @subject.questions.each do |question| %>
        <div class="media">
          <div class="media-body">
            <h6 class="media-heading"><%= question.content %></h6>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
