<%= form_for [:admin, @subject] do |f| %>
  <%= render "shared/error_messages", object: @subject %>

  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>

  <%= f.label :number_question %>
  <%= f.text_field :number_question, class: "form-control" %>

  <%= f.label :duration %>
  <%= f.text_field :duration, class: "form-control" %>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
